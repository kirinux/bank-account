<?xml version="1.0" encoding="UTF-8"?>
<Configuration strict="true">

    <Appenders>
        <Appender type="Console" name="STDOUT">
            <Layout type="PatternLayout" pattern="%d{ISO8601} [%t] %c %m %n"/>
        </Appender>
    </Appenders>

    <Loggers>
        <Root level="DEBUG">
            <AppenderRef ref="STDOUT"/>
        </Root>
    </Loggers>

</Configuration>
